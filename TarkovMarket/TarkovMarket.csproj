<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
		<LangVersion>10</LangVersion>
        <Authors>vrachv</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>TarkovMarket.NET</PackageId>
        <Product>TarkovMarket.NET</Product>
        <PackageTags>tarkov, eft, escapefromtarkov, api</PackageTags>
        <Version>1.3.0</Version>
        <Description>C# wrapper for tarkov-market.com API</Description>
        <RepositoryUrl>https://github.com/vrachv/TarkovMarket.Net</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
      <PackageReference Include="System.Net.Http" Version="4.3.4" />
    </ItemGroup>

</Project>